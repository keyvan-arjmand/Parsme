@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="fa" dir="rtl">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>به پارس می خوش آمدید </title>
    <!-- font---------------------------------------->
    <link rel="stylesheet" href="/assetsCli/css/vendor/font-awesome.min.css">
    <link rel="stylesheet" href="/assetsCli/css/vendor/materialdesignicons.css">
    <!-- plugin------------------/assetsCli-------------->
    <link rel="stylesheet" href="/assetsCli/css/vendor/bootstrap.css">
    <link rel="stylesheet" href="/assetsCli/css/vendor/owl.carousel.min.css">
    <link rel="stylesheet" href="/assetsCli/css/vendor/noUISlider.min.css">
    <!-- main-style--------------/assetsCli-------------->
    <link rel="stylesheet" href="/assetsCli/css/main.css">
</head>
<body>

<!-- login----------------------------------->
<div class="container">
    <div class="row">
        <div class="col-lg">
            <section class="page-account-box">
                <div class="col-lg-6 col-md-6 col-xs-12 mx-auto">
                    <div class="ds-userlogin">
                        <a href="#" class="account-box-logo">Parsme</a>
                        <div class="account-box">
                            <div class="Login-to-account mt-4">
                                <div class="account-box-content">
                                    <h4 class="mb-2">به پارس می خوش آمدید</h4>
                                    <form action="#" class="form-account text-center">
                                        <div class="user-account-welcome">
                                            <img src="/assetsCli/images/man.png">
                                        </div>
                                        <div class="made-account">
                                            <h2>حساب کاربری شما در پارس می ساخته شد</h2>
                                            <p>اکنون می‌توانید به صفحه‌ای که در آن بودید بازگردید و یا با تکمیل اطلاعات حساب کاربری
                                                خود به کلیه امکانات و
                                                سرویس‌های پارس می و سرویس‌های وابسته به آن دسترسی داشته باشید</p>
                                        </div>
                                        <div class="form-row-account">
                                            <button class="btn btn-primary"><a href="@Url.Action("ProfileDetail", "Home")" class="text-decoration-none text-white">تکمیل حساب کاربری</a></button>
                                        </div>
                                    </form>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </section>
        </div>
    </div>
</div>
<!-- login----------------------------------->
                   
<!-- scroll_Progress------------------------->
<div class="progress-wrap">
    <svg class="progress-circle svg-content" width="100%" height="100%" viewBox="-1 -1 102 102">
        <path d="M50,1 a49,49 0 0,1 0,98 a49,49 0 0,1 0,-98"/>
    </svg>
</div>
<!-- scroll_Progress------------------------->

</body>

<!-- file js---------------------------------------------------->
<script src="/assetsCli/js/vendor/jquery-3.2.1.min.js"></script>
<script src="/assetsCli/js/vendor/bootstrap.js"></script>
<!-- plugin--/assetsCli--------------------------------------------->
<script src="/assetsCli/js/vendor/owl.carousel.min.js"></script>
<script src="/assetsCli/js/vendor/jquery.countdown.js"></script>
<script src="/assetsCli/js/vendor/ResizeSensor.min.js"></script>
<script src="/assetsCli/js/vendor/theia-sticky-sidebar.min.js"></script>
<script src="/assetsCli/js/vendor/wNumb.js"></script>
<script src="/assetsCli/js/vendor/nouislider.min.js"></script>
<!-- main js-/assetsCli--------------------------------------------->
<script src="/assetsCli/js/main.js"></script>
<script>
    function Login(){

        $.ajax({
            type: "Post",
            data: {password: $('#password').val(),phoneNumber:$('#phoneNumber').val()},
            url: "/Home/UserLogin",
            success: function (data) {
                window.location.href= "/admin/index";
            },
            error: function (xhr) {
                alert("نام کاربری یا رمز عبور معتبر نمیباشد");
            }
        });
    }
</script>
</html>